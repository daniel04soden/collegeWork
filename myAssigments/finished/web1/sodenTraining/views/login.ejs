
<%- include('partials/head') %> 
        <form action="/users/login" method="post">
            <div class="login-form">
                <label for="name">Username:</label>
                <input type="text" name="username" id="name" placeholder="Enter your name" required autocomplete="off">
            </div>
            <div class="login-form">
                <label for="Password">Password:</label>
                <input type="password" name="password" id="password" placeholder="Password" required>
            </div>
            <button type="submit" class="submission-btn">Login</button>
        </form>
        <p> 
        <a href="/users/register" class="link">No account? Register now</a>
        </p>

        

