<%- include('partials/head') %> 
<h2>Check your bookings below!</h2>
        <div class="book-square">
          <h3>Please provide Personal details below:</h3>
          <hr>

          <form action="/session/manage", method="post">
            <label for="idCheck">Enter your given ID (10 Characters long) below:</label><br>
            <input type="text" id="idCheck" name="idCheck"><br>
            <button type="submit">Submit</button>
          </form>
        </div>
        <div>
         <% if (bookingInfo) { %> 
            <ul>Your Booking Information</ul>
            <li><%= bookingInfo.name %> </li>
            <li><%= bookingInfo.date %> </li>
            <li><%= bookingInfo.time %> </li>
          <% } else { %>
            <p>No booking information available.</p>
          <% } %>
           
        </div>

<%- include('partials/foot') %> 