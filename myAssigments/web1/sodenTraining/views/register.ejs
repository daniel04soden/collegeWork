<%- include('partials/head') %> 
        <form action="" method="post">
            <div class="login-form">
                <label for="name">Username*:</label>
                <input type="text" name="username" id="name" placeholder="Enter your name" required autocomplete="off">
            </div>
           <div class="login-form">
            <label for="email">Email:</label>
            <input type="text" name="email" id="email" placeholder="Enter your email">
           </div> 
            <div class="login-form">
                <label for="Password">Password*:</label>
                <input type="password" name="password" id="password" placeholder="Password" required>
            </div>
            <div class="login-form">
                <label for="age">Age*:</label>
                <input type="number" name="age" id="age" placeholder="00" required>
            </div>
            <div class="login-form">
                <label for="cpassword">Confirm Password*:</label>
                <input type="cpassword" name="cpassword" id="cpassword" placeholder="Confirm Password" required>
            </div>
            
            <button type="submit" class="submission-btn">Sign up</button>
        </form>
        <p> 
        <a href="/session/login" class="link">Already have an account? Login now!</a>
        </p>

<%- include('partials/foot') %> 